{
  "name": "udacity-image-processing-api-ts",
  "version": "1.0.0",
  "description": "Udacity Image Processing API - TypeScript + Jasmine + ESLint + Prettier",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"**/*.md\"",
    "test": "jasmine",
    "test:watch": "jasmine --watch",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
  "@types/express": "^4.17.17",
  "@types/morgan": "^1.9.10",
  "@types/jasmine": "^4.3.1",
  "@types/node": "^20.5.1",
  "@types/supertest": "^2.0.12",
  "cross-env": "^7.0.3",
  "eslint": "^8.50.0",
  "eslint-config-prettier": "^9.0.0",
  "eslint-plugin-prettier": "^5.1.3",
  "@typescript-eslint/eslint-plugin": "^6.10.0",
  "@typescript-eslint/parser": "^6.10.0",
  "husky": "^8.0.3",
  "jasmine": "^5.1.0",
  "jasmine-spec-reporter": "^7.0.0",
  "prettier": "^3.3.3",
  "rimraf": "^5.0.0",
  "supertest": "^6.3.3",
  "ts-node": "^10.9.1",
  "ts-node-dev": "^2.0.0",
  "typescript": "^5.6.0"
}

}